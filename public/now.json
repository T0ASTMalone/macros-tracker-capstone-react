{
  "version": 2,
  "alias": "miguels-macro-tracker",
  "name": "macros-tracker",
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/favicon.ico$",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/manifest.json$",
      "dest": "/manifest.json"
    },
    {
      "src": "^/logo-small.png$",
      "dest": "/logo-small.png",
      "headers": { "Content-Type": "image/png" }
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
